<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace: 패키지경로.인터페이스명 -->
<mapper namespace="com.ezen.myapp.persistance.MemberbService">

	<!-- id: 메소드 이름,	parameterType: 파라미터 타입, #{파라미터명}: 파라미터명 	-->
	<insert id="insertMember" parameterType="String">
		INSERT INTO boardtest(subject) values(#{subject})
	</insert>
	
	<select id="selectAllMember" resultType="mv">
		SELECT * FROM boardtest ORDER BY bidx ASC
	</select>
	
	<select id="selectOneMember" parameterType="String" resultType="mv">
		SELECT * FROM boardtest WHERE bidx = #{bidx}
	</select>
	
	<!--
	<delete id="deleteBoard" parameterType="String">
		SELECT * FROM boardtest WHERE bidx = #{bidx}
	</delete>
	-->

</mapper>